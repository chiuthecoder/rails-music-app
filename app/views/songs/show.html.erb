<div class="col-md-12">

<h3>Others who added <%= @song.artist %></h3>
<h3>song id: <%= @song.id %></h3>
<table class="table table-striped">
<tr>
	<td>Name</td>
	<td>Times Added</td>
</tr>

<% @song.liking_users.group('id').each do |user| %>
<tr>
	<td><a href="/users/<%= user.id %>"><%= user.first_name %> <%= user.last_name %></a></td>
	<td>
		<%= user.likes.where(song: @song).count %>

	</td>
</tr>
<% end %>
Song</table>
</div>

